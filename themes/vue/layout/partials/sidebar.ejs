<% var printedCategories = [] %>
<div class="sidebar">
  <ul class="main-menu">
    <%- partial('partials/main_menu', { context: 'sidebar' }) %>
  </ul>
  <div class="list" style="margin-top: 15px;">
    <ul class="menu-root">
      <% type !== 'api' && site.pages.find({platform:
      platform}).sort('sidebarSorting').each(function (p,index) { %>  <%
      printedCategories.push(p.type) %> <% if (index ==
      site.pages.find({platform: platform}).length - 1){ %> <%
      printedCategories.filter((v,i) => printedCategories.indexOf(v) == i)
      .forEach(function (filteredCategoryName,indexTwo) { %>
      <h3>
        <%-`${filteredCategoryName.slice(0).replace('-', ' ')
        .replace(/(?:^|\s)\S/g, category => category.toUpperCase())}` %>
      </h3>
      <% type !== 'api' && site.pages.find({platform: platform, type:
      filteredCategoryName }).sort('order').each(function (p,index) { %> <% if
      (p.path.startsWith(path.substring(0, 2))) { %> <% var fileName =
      p.path.replace(/^.+?\/([\w-]+)\.html/, '$1') %> <% if (type ===
      'auto-layout') { %> <% if (fileName === '02-installation') { %>
      <li><h3>Essentials</h3></li>
      <% } %> <% if (fileName === '06-orientation') { %>
      <li><h3>Advanced</h3></li>
      <% } %> <% if (fileName === '13-examples') { %>
      <li><h3>Examples</h3></li>
      <% } %> <% } %>
      <li>
        <a
          href="/<%- p.path %>"
          class="sidebar-link<%- page.title === p.title ? ' current' : '' %><%- p.is_new ? ' new' : '' %>"
          ><%- p.title %>
        </a>
      </li>
      <% } %> <% }) %> <% }) %> <% } %> <% }) %>
    </ul>
  </div>
  <div style="font-size: 12px; margin-top: 10px;">
    <p>Built with ❤️ &nbsp in NYC & TLV</p>
  </div>
</div>
